{
  "name": "jordy",
  "version": "0.18.2",
  "description": "typescript based frontend toolkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/thesoncriel/jordy"
  },
  "main": "./esm5/index.js",
  "module": "./esm5/index.js",
  "types": "./libs/index.d.ts",
  "scripts": {
    "compile:esm5": "tsc -b tsconfig.esm5.json",
    "compile:types": "tsc -b tsconfig.types.json",
    "clean": "rimraf cjs esm5 esm5 types libs",
    "build": "npm-run-all clean compile:esm5 compile:types",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prettier": "prettier './packages/**/*.ts'",
    "lint": "eslint 'packages/**'"
  },
  "keywords": [
    "typescript",
    "frontend",
    "toolkit"
  ],
  "author": "Theson",
  "license": "ISC",
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-react": "^2.0.0",
    "axios": "^0.21.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "redux": "^4.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vitest": "^0.22.1"
  },
  "files": [
    "esm5/**/*.js",
    "libs/**/*.d.ts"
  ]
}
