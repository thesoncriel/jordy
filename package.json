{
  "name": "ts-fe-toolkit",
  "version": "0.3.1",
  "description": "typescript based frontend toolkit",
  "main": "./cjs/index.js",
  "module": "./esm5/index.js",
  "types": "./libs/index.d.ts",
  "scripts": {
    "compile:esm5": "tsc -b tsconfig.esm5.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "compile:types": "tsc -b tsconfig.types.json",
    "clean": "rimraf cjs esm5 types libs",
    "build": "npm-run-all clean compile:cjs compile:esm5 compile:types",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier": "prettier './packages/**/*.ts'",
    "lint": "eslint 'packages/**'"
  },
  "keywords": [
    "typescript",
    "frontend",
    "toolkit"
  ],
  "author": "Theson",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.24",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "axios": "^0.21.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "jest": "^27.2.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "files": [
    "cjs/**/*.js",
    "esm5/**/*.js",
    "libs/**/*.d.ts"
  ]
}
